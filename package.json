{
  "name": "webpack-repo",
  "version": "1.0.0",
  "description": "prepearing for developing with webpack",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:watch": "jest --watch",
    "build:styles": "sass ./src/styles/styles.css ./dist/styles.css",
    "copy:html": "cp ./src/index.html ./dist/index.html",
    "copy:scripts": "cp -R ./src/scripts ./dist/scripts",
    "clean": "rm -rf ./dist",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d biuld"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaliilatka/webpack-repo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vitaliilatka/webpack-repo/issues"
  },
  "homepage": "https://github.com/vitaliilatka/webpack-repo",
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "sass": "^1.52.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

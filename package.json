{
  "name": "webpack-repo",
  "version": "1.0.0",
  "description": "prepearing for developing with webpack",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build:styles": "sass ./src/styles/styles.css ./dist/styles.css",
    "copy:html": "cp ./src/index.html ./dist/index.html",
    "copy:scripts": "cp -R ./src/scripts ./dist/scripts",
    "clean": "rm -rf ./dist",
    "build": "npm-run-all clean build:styles copy:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitaliilatka/webpack-repo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vitaliilatka/webpack-repo/issues"
  },
  "homepage": "https://github.com/vitaliilatka/webpack-repo#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.52.3"
  }
}
